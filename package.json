{
  "name": "wye",
  "displayName": "Wye",
  "description": "wye change theme~",
  "version": "0.6.0",
  "icon": "icon.jpg",
  "publisher": "phyzess",
  "license": "MIT",
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "wye",
    "light",
    "dark"
  ],
  "files": [
    "themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Wye Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/wye-dark.json"
      },
      {
        "label": "Wye Light",
        "uiTheme": "vs",
        "path": "./themes/wye-light.json"
      },
      {
        "label": "Wye Black",
        "uiTheme": "vs-dark",
        "path": "./themes/wye-black.json"
      },
      {
        "label": "Wye Light Soft",
        "uiTheme": "vs",
        "path": "./themes/wye-light-soft.json"
      },
      {
        "label": "Wye Dark Soft",
        "uiTheme": "vs-dark",
        "path": "./themes/wye-dark-soft.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:phyzess/vscode-theme-wye.git"
  },
  "scripts": {
    "build": "esno src/index.ts",
    "dev": "nodemon --watch src -e ts --exec \"esno src/index.ts\"",
    "vscode:prepublish": "pnpm run build",
    "release": "pnpm run build && pnpm dlx bumpp --commit --tag --push --all && vsce publish --no-dependencies"
  },
  "devDependencies": {
    "@types/node": "^20.5.7",
    "@vscode/vsce": "^2.20.1",
    "esno": "^0.17.0",
    "nodemon": "^3.0.1",
    "typescript": "^5.2.2"
  }
}
